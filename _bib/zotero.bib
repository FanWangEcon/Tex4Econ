	volume = {97},
	issn = {0304-3878},
	doi = {10.1016/j.jdeveco.2011.06.001},
	shorttitle = {Do interest rates matter?},
	abstract = {“Best practice” in microfinance holds that interest rates should be set at profit-making levels, based on the belief that even poor customers favor access to finance over low fees. Despite this core belief, little direct evidence exists on the price elasticity of credit demand in poor communities. We examine increases in the interest rate on microfinance loans in the slums of Dhaka, Bangladesh. Using unanticipated between-branch variation in prices, we estimate interest elasticities from −\&\#xa0;0.73 to −\&\#xa0;1.04, with our preferred estimate being at the upper end of this range. Interest income earned from most borrowers fell, but interest income earned from the largest increased, generating overall profitability at the branch level.},
	pages = {437--449},
	number = {2},
	journaltitle = {Journal of Development Economics},
	author = {Dehejia, Rajeev and Montgomery, Heather and Morduch, Jonathan},
	urldate = {2012-07-19},
