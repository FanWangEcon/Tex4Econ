	date = {2011},
	keywords = {Capital-account liberalization, Current-account deficits, Entrepreneurship, Financial integration, Global imbalances, Idiosyncratic risk, Incomplete markets}
}

@article{zhu_trade_2005,
	title = {Trade and inequality in developing countries: A general equilibrium analysis},
	volume = {65},
	issn = {00221996},
	doi = {10.1016/j.jinteco.2003.11.005},
	abstract = {Developing and newly industrialized countries that have experienced the sharpest increases in wage inequality are those whose export shares have shifted towards more skill-intensive goods. We argue that this can be explained by technological catch-up. We develop this insight using a model that features both Ricardian and endowments-based comparative advantage. In this model, Southern catch-up causes production of the least skill-intensive Northern goods to migrate South (where they become the most skill-intensive Southern goods). This raises wage inequality in both the South and the North. We provide empirical evidence that strongly supports this causal mechanism: Southern catch-up exacerbates Southern inequality by redirecting Southern export shares towards more skill-intensive goods. © 2004 Elsevier B.V. All rights reserved.},
	pages = {21--48},
	number = {1},
	journaltitle = {Journal of International Economics},
	author = {Zhu, Susan Chun and Trefler, Daniel},
	date = {2005},
	keywords = {Inequality, International trade, Southern catch-up}
}

